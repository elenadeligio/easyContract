        <div class="text-left">
            <a href="/gmbhtexts/new" class="btn btn-primary"><i class="fa fa-plus-circle"></i> | Baustein hinzufügen</a>
        </div>
        <br>

        <div id="accordion">
            <!-- Suchfunktion -->
            <div class="input-group margin-bottom-sm">
                <span class="input-group-addon"><i class="fa fa-search" aria-hidden="true"></i></span>
                <input type="text" id="myInput" class="form-control" placeholder="Suche.." autofocus required>
            </div>

            <!-- §1 -->
            <div class="card">
                <div class="card-header" id="heading_1">
                    <h5 class="mb-0">
                        <button class="btn btn-link collapsed" draggable="true" data-toggle="collapse"
                                data-target="#collapse_1" aria-expanded="false" aria-controls="collapse_1">§ 1 Firma und Sitz </button>
                    </h5>
                </div>
                <div id="collapse_1" class="collapse" aria-labelledby="heading_1" data-parent="#accordion">
                    <div class="card-body scrollable" id="myDIV"> <!-- myDIV id for Search function -->
                        <% gmbhtexts.forEach(gmbhtext => { %>
                        <%if (gmbhtext.category == "§ 1 Firma und Sitz") { %>
                            <div class="alert alert-info">
                                <a href="<%= `gmbhtexts/${gmbhtext._id}/delete?_method=DELETE` %>" onclick="return confirm('Wollen Sie es wirklich löschen?')" ><i class="fa fa-times"></i></a>
                                <a href="<%=`gmbhtexts/${gmbhtext._id}/edit` %>"><i class="fa fa-pencil"></i></a>
                                <div class="scrollable" draggable="true" style="height:80px;"><%= gmbhtext.gmbhtext %> </div>
                            </div>
